    "@material-ui/icons": "^4.2.1",
    "@types/crypto-js": "^4.2.2",
    "crypto-js": "^4.2.0",
    "mobx": "^5.13.0",
    "mobx-react": "^6.1.3",
    "nvm": "^0.0.4",
    "react": "^16.9.0",
