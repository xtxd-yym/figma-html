  "license": "MIT",
  "devDependencies": {
    "@types/chrome": "^0.0.88",
    "@types/fs-extra-promise": "^1.0.8",
    "@types/node": "^18.11.18",
    "@types/react": "^16.9.2",
    "@types/react-dom": "^16.9.0",
    "archiver": "^7.0.1",
    "copy-webpack-plugin": "^6.4.1",
    "css-loader": "^3.2.0",
    "fs-extra-promise": "^1.0.1",
    "html-webpack-plugin": "^4.5.2",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.0.0",
    "ts-loader": "^8.3.0",
    "tslint-config-airbnb": "^5.11.1",
    "tslint-react": "^4.0.0",
    "typescript": "^4.9.5",
    "webpack": "^4.46.0",
    "webpack-cli": "^3.3.12",
    "webpack-merge": "^4.2.2",
    "yargs": "^17.7.2"
